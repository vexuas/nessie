{
  "name": "nessie",
  "version": "1.10.5",
  "description": "Apex Legends Map Rotation Discord Bot",
  "scripts": {
    "start": "yarn build && node dist/App.js",
    "build": "tsc",
    "auto:version": "yarn version --`auto version`",
    "update:version": "node -p \"'export const BOT_VERSION = ' + JSON.stringify(require('./package.json').version) + ';' + 'export const BOT_UPDATED_AT = ' + JSON.stringify('$(date +'%d-%b-%Y')')\" > src/version.ts"
  },
  "main": "index.js",
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "axios": "^0.22.0",
    "cron": "^1.8.2",
    "date-fns": "^2.24.0",
    "discord-api-types": "^0.37.43",
    "discord.js": "13.3.1",
    "lodash": "^4.17.21",
    "mixpanel": "^0.13.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "pg": "^8.7.3",
    "sqlite3": "^5.1.5",
    "topgg-autoposter": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cron": "^2.0.1",
    "@types/lodash": "^4.14.195",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "auto": "^10.44.0",
    "eslint": "^8.42.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
